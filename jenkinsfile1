pipeline 
{
   agent any

   stages 
   {
      stage('Build') 
        {
            steps 
            {
                echo 'Building...jenkings directscript'
                echo "Running ${env.BUILD_ID} ${env.BUILD_DISPLAY_NAME} on ${env.NODE_NAME} and JOB ${env.JOB_NAME}"
            }
        }
        stage('Test') 
        {
            steps 
            {
                echo 'Testing...jenkings directscript'
            }
        }
        stage('Deploy') 
        {
            steps 
            {
                echo 'Deploying...jenkings directscript'
            }
        }
    }
}
